class Harshad
	{
	public static void main(String args[])
	{
Scanner sc=new Scanner(System.in);
 int n=sc.nextInt();
 int h;
 int num= n;
 int sum=0;
while(num>0)
 {
 	int r=num%10;
 	sum=sum+r;
 	num=num/10;
 } 
  h= (n%sum);
 if (h!=0)
 {
System.out.println("not a Harshad number");
}else  
{
System.out.println("a Harshad number");
}
}
}
